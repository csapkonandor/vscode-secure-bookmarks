{
  "name": "secure-bookmarks",
  "displayName": "Secure Bookmarks",
  "description": "Simple self-contained bookmark extension",
  "version": "0.0.2",
  "publisher": "csapkonandor",
  "engines": {
    "vscode": "^1.80.0"
  },
  "main": "extension.js",
  "activationEvents": [
    "onCommand:secureBookmarks.add",
    "onCommand:secureBookmarks.goto",
    "onCommand:secureBookmarks.delete",
    "onCommand:secureBookmarks.deleteAll"
  ],
  "contributes": {
    "commands": [
      {
        "command": "secureBookmarks.add",
        "title": "Secure Bookmarks: Add Bookmark"
      },
      {
        "command": "secureBookmarks.goto",
        "title": "Secure Bookmarks: Go to Bookmark"
      },
      {
        "command": "secureBookmarks.delete",
        "title": "Secure Bookmarks: Delete Bookmark"
      },
      {
        "command": "secureBookmarks.deleteAll",
        "title": "Secure Bookmarks: Delete All Bookmarks"
      }
    ]
  }
}

