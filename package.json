{
  "name": "secure-bookmarks",
  "displayName": "Secure Bookmarks",
  "description": "Simple self-contained bookmark extension",
  "version": "0.0.3",
  "publisher": "local",
  "engines": {
    "vscode": "^1.50.0"
  },
  "main": "extension.js",
  "activationEvents": [
    "onCommand:secureBookmarks.add",
    "onCommand:secureBookmarks.goto",
    "onCommand:secureBookmarks.delete",
    "onCommand:secureBookmarks.deleteAll",
    "onCommand:secureBookmarks.next",
    "onCommand:secureBookmarks.previous"
  ],
  "contributes": {
    "commands": [
      { "command": "secureBookmarks.add", "title": "Secure Bookmarks: Add Bookmark" },
      { "command": "secureBookmarks.goto", "title": "Secure Bookmarks: Go to Bookmark" },
      { "command": "secureBookmarks.delete", "title": "Secure Bookmarks: Delete Bookmark" },
      { "command": "secureBookmarks.deleteAll", "title": "Secure Bookmarks: Delete All Bookmarks" },
      { "command": "secureBookmarks.next", "title": "Secure Bookmarks: Go to Next Bookmark" },
      { "command": "secureBookmarks.previous", "title": "Secure Bookmarks: Go to Previous Bookmark" }
    ]
  }
}

